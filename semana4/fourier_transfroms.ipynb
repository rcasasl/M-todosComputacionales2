   "source": [
    "def DTF_lenta(x):\n",
    "    N = len(x)\n",
    "    X = np.zeros(len(N), complex)\n",
    "    for k in range(N):\n",
    "        for n in range(N):\n",
    "            X[k] += (1/N)*(x[n])*np.exp(-1j*n*k/N)\n",
    "    return X"
   ]
  }
